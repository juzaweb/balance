{
    "name": "juzaweb/balance",
    "homepage": "https://juzaweb.com",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Add balance for user Juzaweb CMS",
    "extra": {
        "juzaweb": {
            "name": "Juzaweb Balance",
            "domain": "balance",
            "cms_min": "3.3",
            "version": "1.0",
            "providers": [
                "Juzaweb\\Balance\\Providers\\BalanceServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Balance\\": "src/"
        }
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    }
}
